{
  "all": true,
  "include": [
    "labs/**/*.js",
    "scripts/**/*.js"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/test/**",
    "**/tests/**",
    "**/*.test.js",
    "**/*.spec.js",
    "**/coverage/**",
    "**/dist/**"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "reports-dir": "./coverage",
  "temp-directory": "./.nyc_output",
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
